<div class="text-center">
  <h1>Add new material</h1>
</div>
<div>
  <%= form_for(@material_need) do |form| %>
    <%= render 'create_material_need_fields', form: form %>
    <%= hidden_field_tag 'project_id', @project_id %>  
    <%= form.submit "Add material", class: "btn btn-large btn-primary" %>
  <% end %>
</div>
